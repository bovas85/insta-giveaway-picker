{
  "name": "insta-analyzer",
  "version": "1.0.0",
  "description": "Instagram Comment Analyzer",
  "main": "index.ts",
  "scripts": {
    "serve": "ts-node src/server/index.ts",
    "build": "npm run build:css && tsc && npm run scan",
    "scan": "node scripts/scan.js . --fail-on=warning",
    "start": "node dist/server/index.js",
    "test": "jest",
    "lint": "eslint . --ext .ts --format codeframe",
    "format": "prettier --write .",
    "build:css": "tailwindcss -i ./src/client/input.css -o ./src/client/output.css",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,js,json,md}": [
      "prettier --write"
    ],
    "*.ts": [
      "eslint --fix"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/dotenv": "6.1.1",
    "@types/jest": "29.5.14",
    "@types/node": "20.19.27",
    "@types/puppeteer": "5.4.7",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "autoprefixer": "10.4.23",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.2",
    "eslint-formatter-codeframe": "7.32.2",
    "eslint-plugin-prettier": "5.5.4",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "lint-staged": "15.5.2",
    "postcss": "8.5.6",
    "prettier": "3.7.4",
    "socket.io-client": "4.8.1",
    "supertest": "7.0.0",
    "tailwindcss": "3.4.19",
    "ts-jest": "29.4.6",
    "ts-node": "10.9.2",
    "typescript": "5.5.4"
  },
  "dependencies": {
    "@types/express": "5.0.6",
    "@types/socket.io": "3.0.1",
    "axios": "1.13.2",
    "dotenv": "17.2.3",
    "express": "5.2.1",
    "puppeteer": "24.34.0",
    "socket.io": "4.8.3"
  }
}
